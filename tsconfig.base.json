{
	"compilerOptions": {
		"forceConsistentCasingInFileNames": true,
		"importHelpers": true,
		"allowJs": false,
		"allowSyntheticDefaultImports": true,
		"declaration": true,
		"esModuleInterop": true,
		"lib": ["dom", "es2017"],
		"module": "commonjs",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"sourceMap": true,
		"strict": true,
		"target": "ES6",
		"types": ["node"],
		"skipLibCheck": true
	}
}
